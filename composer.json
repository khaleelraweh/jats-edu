{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1.6|^8.2.12",
        "altwaireb/laravel-world": "^1.0",
        "barryvdh/laravel-debugbar": "^3.9",
        "codezero/laravel-unique-translation": "^4.2",
        "cviebrock/eloquent-sluggable": "^10.0",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "jantinnerezo/livewire-alert": "^3.0",
        "khaled.alshamaa/ar-php": "^6.3",
        "laravel/framework": "^10.48.25",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "livewire/livewire": "^2.12",
        "mindscms/entrust": "^2.6",
        "mindscms/laravelshoppingcart": "^2.1",
        "nicolaslopezj/searchable": "^1.13",
        "paytabscom/laravel_paytabs": "^1.7",
        "predis/predis": "^2.2",
        "pusher/pusher-php-server": "^7.2",
        "realrashid/sweet-alert": "^7.1",
        "spatie/laravel-sluggable": "^3.5",
        "spatie/laravel-translatable": "^6.5",
        "srmklive/paypal": "~3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.39",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helper/GeneralHelper.php"
        ]

    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
